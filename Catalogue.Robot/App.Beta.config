<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="Data" connectionString="Url=http://localhost:8090/;Database=Catalogue.Data.Beta" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
  </connectionStrings>
  
  <appSettings>
    <add key="Environment" value="Beta" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>

  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="C:\topcat\Beta\logs\topcat-robot" />
    </appender>
  </log4net>
  
</configuration>